    itunesRefreshInterval: 100
    showEqualizer: false
    showNotes: true
  "atom-jest-snippets":
    toggleSemicolons: false
  "atom-minify":
    jsMinifier: "UglifyJS2"
  "atom-minimap-titles":
    font: "ANSI Shadow"
  "atom-package-deps":
