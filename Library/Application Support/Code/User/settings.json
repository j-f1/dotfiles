{
  "editor.minimap.enabled": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": "'zero', 'onum', 'ss04' /* $ */, 'ss06' /* \\ */",
  "window.autoDetectColorScheme": true,
  "window.clickThroughInactive": false,
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.associations": {
    "*.rkt": "alloy",
    ".netlify.toml": "toml"
  },
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.nextEditorInGroup",
    "workbench.action.previousEditorInGroup"
  ],
  "terminal.integrated.env.osx": {
    "EDITOR": "code -rw",
    "TERM_PROGRAM": "vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "workbench.editorAssociations": [
    {
      "viewType": "jupyter.notebook.ipynb",
      "filenamePattern": "*.ipynb"
    }
  ],
  "python.showStartPage": false,
  "explorer.confirmDragAndDrop": false,
  "search.exclude": {
    "**/site-packages/**": true,
    "/var/**": true
  },
  "search.globalFindClipboard": true,
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "customizeUI.activityBar": "top",
  "customizeUI.titleBar": "inline",
  "customizeUI.stylesheet": {
    ".notifications-toasts, .notifications-center": "bottom: auto !important; top: var(--traffic-lights-height); margin-top: var(--activity-bar-action-height);",
    ".codicon-notifications-collapse, .codicon-notifications-expand, .notifications-center [title*='Hide Notifications']": "transform: scaleY(-1)",
    // ".notifications-center [title*='Hide Notifications']::before": "content: '\\ea76' !important;",
    ".monaco-workbench>.notifications-toasts .notification-toast-container>.notification-toast": "transform: translate3d(0,-100%,0)",
    ".view-line *": "font-style: normal !important",
    ".vs [style='color: rgb(255, 255, 255);'], .vs #workbench\\.parts\\.activitybar .action-item:hover [style^=color][style*='(255, 255, 255']:not(.badge-content)": "color: #424242 !important",
    ".vs #workbench\\.parts\\.activitybar .action-item:hover [style^=background-color][style*='(255, 255, 255']:not(.badge-content)": "background-color: #424242 !important",
    ".vs [style='background-color: rgba(255, 255, 255, 0.4);']:not(.select-box-dropdown-list-container):not(.badge-content), .vs [style='background-color: rgb(255, 255, 255);']:not(.select-box-dropdown-list-container):not(.badge-content)": "background-color: rgba(66, 66, 66, 0.4) !important",
    ".vs .activitybar": "background: transparent !important",
    ".composite.title .title-actions > * > * > .actions-container > .action-item:not(.grow) [role=button]:not(.disabled):hover::after": "content: ''; width: 24px; height: 24px; background: var(--background-highlight); display: inline-block; margin-left: -20px; margin-right: -4px; top: 4px; z-index: -1; border-radius: 50%; position: relative;",
    "[role=application].vs-dark": "--background-highlight: #444",
    "[role=application]": "--background-highlight: #ddd; margin-top: var(--traffic-lights-height); height: calc(100vh - var(--traffic-lights-height) + 22px) !important;",
    ".activitybar .content": "height: auto !important;",
    ".dragging-area-top": "-webkit-app-region: no-drag !important;",
    // ".dragging-area-left-padding": "width: 0 !important;",
    "[aria-label='Remote Host']": "display: none !important",
    ".monaco-workbench .part.statusbar.part.statusbar": "top: 0 !important; position: fixed !important; height: var(--traffic-lights-height) !important; background: transparent !important; color: inherit !important;",
    ".nosidebar .part.editor": "top: -28px",
    // ".nosidebar .part.statusbar": "padding-left: calc(3px + var(--traffict-lights-width));",
    ".part.statusbar": "padding: 0 7px; padding-left: calc(3px + var(--traffict-lights-width)); -webkit-app-region: drag; outline: none !important",
    ".statusbar-item, .statusbar-item": "padding-top: 7px; padding-bottom: 7px; height: calc(100% - 14px) !important; -webkit-app-region: no-drag;",
    ".statusbar-item a:hover, .statusbar-item a:focus": "border-radius: 5px; background: var(--background-highlight) !important;",
    ".monaco-workbench .part.statusbar>.items-container>.statusbar-item.right.last-visible-item": "padding-right: 0 !important;",
    ".monaco-workbench .part.statusbar > .items-container > .statusbar-item.has-beak:before": "display: none",
    ".activitybar *": "cursor: auto !important;",
    ".activitybar .composite-bar": "width: 100%",
    ".activitybar .actions-container": "justify-content: space-between !important",
    ".sidebar, .sidebar > *": "transform: scaleY(-1)",
    // ".sidebar .composite.title": "position: absolute; bottom: 15px; width: 100%",
    ".sidebar .composite.title[style*='rgb(234, 234, 235)']": "background: #f3f3f3 !important",
    ".sidebar .composite.title[style*='rgb(33, 37, 43)']": "background: #252526 !important"
  },
  "liveshare.showInStatusBar": "whileCollaborating",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.enableSmartCommit": true,
  "workbench.colorTheme": "Atom One Light",
  "editor.renderWhitespace": "none",
  "editor.wordWrap": "on",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.foldingHighlight": false,
  "editor.inlineHints.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.linkedEditing": true,
  "editor.renderFinalNewline": false,
  "editor.renderControlCharacters": true,
  "editor.scrollBeyondLastLine": false,
  "editor.wrappingIndent": "indent",
  "editor.cursorSurroundingLines": 3,
  "editor.find.globalFindClipboard": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.suggest.localityBonus": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggest.insertMode": "replace",
  "editor.suggest.showStatusBar": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "workbench.commandPalette.preserveInput": true,
  "workbench.quickOpen.preserveInput": true,
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.preferredDarkColorTheme": "Atom One Dark",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "editor.fontWeight": 500,
  "editor.padding.bottom": 100,
  "workbench.fontAliasing": "auto",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.untitled.hint": "button",
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "alphabetical",
  "search.actionsPosition": "auto",
  "search.quickOpen.includeSymbols": true,
  "scm.alwaysShowActions": true,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "terminal.integrated.automationShell.osx": "/bin/zsh",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.fontWeight": 500,
  "terminal.integrated.minimumContrastRatio": 4.5,
  "license.author": "Jed Fox",
  "css.lint.compatibleVendorPrefixes": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.ieHack": "warning",
  "css.lint.zeroUnits": "warning",
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "git.allowNoVerifyCommit": true,
  "git.autofetch": false,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "git.confirmSync": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    // until https://github.com/microsoft/vscode-python/issues/1883 is fixed
    "editor.formatOnPaste": false
  },
  "zenMode.hideStatusBar": false,
  "zenMode.silentNotifications": false,
  "editor.accessibilitySupport": "off",
  "terminal.integrated.inheritEnv": false,
  "python.formatting.provider": "black",
  "window.titleBarStyle": "native",
  "editor.formatOnSave": true,
  "inline-parameters.fontStyle": "normal",
  "inline-parameters.hideSingleParameters": true,
  "customizeUI.statusBarPosition": "top"
}
